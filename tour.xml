<!--这是户型图可以拖拽动作-->
<action name="floorplansize_drag">
copy(floorplandragebeforewidth,layer[floorplan].width);
calc(mergedrag,floorplandragebeforewidth-(stagewidth-mouse.stagex));
asyncloop(pressed,
	calc(floorplanwidth,stagewidth-mouse.stagex+mergedrag);
    calc(floorplanheight,(stagewidth-mouse.stagex+mergedrag)*0.7257);
   asyncloop((floorplanwidth GE 250) AND (floorplanwidth LT 800) AND pressed ,
	calc(layer[floorplan].width,stagewidth-mouse.stagex+mergedrag);
    calc(layer[floorplan].height,(stagewidth-mouse.stagex+mergedrag)*0.7257);
     );
   asyncloop(floorplanwidth LT 250 AND pressed,
	calc(layer[floorplan].width,150+100);
    calc(layer[floorplan].height,180.425+1);
     );
   asyncloop(floorplanwidth GE 800 AND pressed,
	calc(layer[floorplan].width,800);
    calc(layer[floorplan].height,580.56);
     );


	);
</action>

<action name="tip">

copy(layer[tiptip].html,tooltip);
set(layer[tiptip].visible,true);
asyncloop(tooltip !== null,
	calc(layer[tiptip].x,mouse.stagex+15);
    calc(layer[tiptip].y,mouse.stagey+15);
   );
</action>

<!--这是移动手机缩略图点扩大碰触添加图层动作-->
<action name="addpointtouchlarger">
for(set(f,0), f LT 10, inc(f),
			calc(pointlargername,'pointtouchlarger_' + f);
			calc(pointlargername_parent,'floorpoint_'+f);
			addlayer(get(pointlargername));
			set(layer[get(pointlargername)].keep, true);
			set(layer[get(pointlargername)].width, 40);
			set(layer[get(pointlargername)].height, 40);
			copy(layer[get(pointlargername)].parent, pointlargername_parent);
			copy(layer[get(pointlargername)].url, layer[floorpoint_0].url);
			set(layer[get(pointlargername)].align,center);
			set(layer[get(pointlargername)].linkedscene, get(scene[get(f)].name) );
			set(layer[get(pointlargername)].onclick,loadscene(get(linkedscene), null, MERGE, BLEND(0.5));caonimabi(get(linkedscene));worinima(););
			set(layer[get(pointlargername)].enabled,true);
			set(layer[get(pointlargername)].alpha,0.0);

			

    );
</action>

